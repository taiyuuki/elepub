{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "elepub",
    "version": "3.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true,
        "open": true,
        "save": true
      },
      "protocol": {
        "all": false,
        "asset": true,
        "assetScope": [
          "$PICTURE"
        ]
      },
      "fs": {
        "all": false,
        "copyFile": false,
        "createDir": false,
        "exists": true,
        "readDir": false,
        "readFile": false,
        "removeDir": false,
        "removeFile": false,
        "renameFile": false,
        "scope": [
          "**"
        ],
        "writeFile": false
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.elepub",
      "targets": "all",
      "windows": {
        "wix": {
          "language": "zh-CN"
        }
      }
    },
    "security": {
      "csp": ""
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "多看漫画制作",
        "width": 1024,
        "height": 600
      }
    ]
  }
}